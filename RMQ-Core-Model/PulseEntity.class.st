Class {
	#name : #PulseEntity,
	#superclass : #MooseEntity,
	#instVars : [
		'pulseName',
		'sources'
	],
	#category : #'RMQ-Core-Model'
}

{ #category : #meta }
PulseEntity class >> annotation [

	<MSEClass: #MooseEntity super: #PulseEntity>
	<package: #Pulse>
	^self
]

{ #category : #testing }
PulseEntity class >> isVoyageRoot [

	^true
]

{ #category : #accessing }
PulseEntity >> initialize [

	super initialize.	
	sources := Set new.
]

{ #category : #accessing }
PulseEntity >> name [
	<MSEProperty: #name type: #String>
	^ self pulseName
]

{ #category : #accessing }
PulseEntity >> pulseName [
	^ pulseName
]

{ #category : #accessing }
PulseEntity >> pulseName: anObject [
	pulseName := anObject
]

{ #category : #utility }
PulseEntity >> selectEnum: anEnumObject [
	^ self mooseModel
		selectOrAddEntityNamed: anEnumObject name
		withType: anEnumObject class
		sourceName: 'none'
]

{ #category : #accessing }
PulseEntity >> sources [
	^ sources
]

{ #category : #accessing }
PulseEntity >> sources: anObject [
	sources := anObject
]
